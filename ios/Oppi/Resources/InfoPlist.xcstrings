    "CFBundleDisplayName": {
      "localizations": {
        "en": {
          "stringUnit": {
            "state": "translated",
            "value": "Oppi"
          }
        },
        "zh-Hans": {
          "stringUnit": {
            "state": "translated",
            "value": "Oppi"
          }
        }
      }
    },
    "NSCameraUsageDescription": {
      "localizations": {
        "en": {
          "stringUnit": {
            "state": "translated",
            "value": "Oppi uses the camera to take photos for agent conversations."
          }
        },
        "zh-Hans": {
          "stringUnit": {
            "state": "translated",
            "value": "Oppi 使用相机为智能体对话拍摄照片。"
          }
        }
      }
    },
    "NSFaceIDUsageDescription": {
      "localizations": {
        "en": {
          "stringUnit": {
            "state": "translated",
            "value": "Oppi uses Face ID to confirm approval of high-risk agent actions like sudo, force push, and recursive deletes."
          }
        },
        "zh-Hans": {
          "stringUnit": {
            "state": "translated",
            "value": "Oppi 使用面容 ID 确认批准高风险智能体操作，例如 sudo、强制推送和递归删除。"
          }
        }
      }
    },
    "NSLocalNetworkUsageDescription": {
      "localizations": {
        "en": {
          "stringUnit": {
            "state": "translated",
            "value": "Oppi needs local network access to connect to your Mac server."
          }
        },
        "zh-Hans": {
          "stringUnit": {
            "state": "translated",
            "value": "Oppi 需要本地网络访问权限以连接到你的 Mac 服务器。"
          }
        }
      }
    },
    "NSMicrophoneUsageDescription": {
      "localizations": {
        "en": {
          "stringUnit": {
            "state": "translated",
            "value": "Oppi uses the microphone to transcribe your voice into text prompts."
          }
        },
        "zh-Hans": {
          "stringUnit": {
            "state": "translated",
            "value": "Oppi 使用麦克风将你的语音转录为文本提示词。"
          }
        }
      }
    },
    "NSPhotoLibraryAddUsageDescription": {
      "localizations": {
        "en": {
          "stringUnit": {
            "state": "translated",
            "value": "Oppi saves images from agent conversations to your photo library."
          }
        },
        "zh-Hans": {
          "stringUnit": {
            "state": "translated",
            "value": "Oppi 将智能体对话中的图片保存到你的相册。"
          }
        }
      }
    },
    "NSPhotoLibraryUsageDescription": {
      "localizations": {
        "en": {
          "stringUnit": {
            "state": "translated",
            "value": "Oppi accesses your photo library to attach images to agent conversations."
          }
        },
        "zh-Hans": {
          "stringUnit": {
            "state": "translated",
            "value": "Oppi 访问你的相册以将图片附加到智能体对话中。"
          }
        }
      }
    },
    "NSSpeechRecognitionUsageDescription": {
      "localizations": {
        "en": {
          "stringUnit": {
            "state": "translated",
            "value": "Oppi uses on-device speech recognition to convert your voice into text."
          }
        },
        "zh-Hans": {
          "stringUnit": {
            "state": "translated",
            "value": "Oppi 使用设备端语音识别将你的语音转换为文本。"
          }
        }
      }
    }
  }
}
